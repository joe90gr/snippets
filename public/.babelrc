{
	"presets": [
	  "es2015",
	  "react"
	],
	"plugins": [
		["module-resolver", {
				"root": ["./src"],
				"alias": {
					"src": "./src" ,
					"actions": "./src/actions",
					"common": "./src/common",
					"configuration": "./src/configuration",
					"constants": "./src/constants",
					"mixins": "./src/mixins",
					"routes": "./src/routes",
					"services": "./src/services",
					"stores": "./src/stores",
					"utils": "./src/utils",
					"views": "./src/views",
				}
			}
		]
	]
}
